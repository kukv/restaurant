<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.kukv.reservations.infrastructure.datasource.reservation.course.CourseRecordMapper">
    <insert id="record">
        insert into reservations.reservation_course(reservation_id, course_name, created_at)
        values (#{reservationId.value}, #{course.courseName.value}, #{createdTime.value})
    </insert>
</mapper>
